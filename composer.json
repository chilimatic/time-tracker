{
  "name" : "chilimatic/time-tracker",
  "description" : "small time-tracker",
  "version": "0.1.0",
  "license" : "MIT",
  "repositories": [
    {
      "type": "git",
      "url" : "git@github.com:chilimatic/chilimatic-framework.git"
    }
  ],
  "require": {
    "php": ">=7",
    "chilimatic/framework" : "dev-feature/compoment-splitting",
    "chilimatic/interfaces-component" : "2.*",
    "chilimatic/transformer-component": "2.*",
    "chilimatic/parser-component": "2.*",
    "chilimatic/di-component": "2.*",
    "chilimatic/cache-component": "2.*",
    "chilimatic/database-component": "dev-master",
    "robmorgan/phinx": "0.5.*"
  },
  "config": {
    "bin-dir": "bin/",
    "vendor-dir": "vendor"
  },
  "minimum-stability" : "dev",
  "autoload": {
    "psr-4": {
      "timetracker\\app\\": "app/"
    }
  }
}
